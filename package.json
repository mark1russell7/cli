{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/cli",
  "version": "1.0.0",
  "description": "Mark CLI - Development workflow automation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mark": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo",
    "prepare": "npm run build",
    "postinstall": "client announce"
  },
  "dependencies": {
    "@mark1russell7/client": "github:mark1russell7/client#main",
    "@mark1russell7/client-cli": "github:mark1russell7/client-cli#main",
    "@mark1russell7/client-logger": "github:mark1russell7/client-logger#main",
    "commander": "^13.0.0",
    "gluegun": "^5.2.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@mark1russell7/cue": "github:mark1russell7/cue#main",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cli",
    "mark",
    "development",
    "workflow",
    "npm",
    "refresh"
  ],
  "license": "MIT",
  "sideEffects": false
}
