{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/cli",
  "version": "1.0.0",
  "description": "Mark CLI - Development workflow automation",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mark": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo",
    "prepare": "npm run build",
    "test:e2e": "vitest run e2e/",
    "test:e2e:watch": "vitest e2e/",
    "postinstall": "client announce"
  },
  "dependencies": {
    "@mark1russell7/client": "github:mark1russell7/client#main",
    "@mark1russell7/client-cli": "github:mark1russell7/client-cli#main",
    "@mark1russell7/client-lib": "github:mark1russell7/client-lib#main",
    "@mark1russell7/client-logger": "github:mark1russell7/client-logger#main",
    "@mark1russell7/client-pnpm": "github:mark1russell7/client-pnpm#main",
    "@mark1russell7/client-procedure": "github:mark1russell7/client-procedure#main",
    "@mark1russell7/client-shell": "github:mark1russell7/client-shell#main",
    "@mark1russell7/client-test": "github:mark1russell7/client-test#main",
    "@mark1russell7/client-vitest": "github:mark1russell7/client-vitest#main",
    "gluegun": "^5.2.2",
    "tslib": "^2.8.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@mark1russell7/cue": "github:mark1russell7/cue#main",
    "@mark1russell7/test": "github:mark1russell7/test#main",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "cli",
    "mark",
    "development",
    "workflow",
    "npm",
    "refresh"
  ],
  "license": "MIT",
  "sideEffects": false,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@mark1russell7/client",
      "@mark1russell7/client-cli",
      "@mark1russell7/client-fs",
      "@mark1russell7/client-git",
      "@mark1russell7/client-lib",
      "@mark1russell7/client-logger",
      "@mark1russell7/client-pnpm",
      "@mark1russell7/client-procedure",
      "@mark1russell7/client-shell",
      "@mark1russell7/client-test",
      "@mark1russell7/client-vitest",
      "@mark1russell7/cue",
      "@mark1russell7/test",
      "esbuild"
    ]
  }
}
